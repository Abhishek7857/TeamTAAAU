@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <!-- Bootstrap JS -->
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</head>

<body>
    <div>
        <div class="navbar navbar-inverse navbar-fixed-top shadow p-3 mb-5 rounded text-white" style="background-color:deepskyblue; ">
            <div class="container">
                <span class="navbar-header" style="color:ghostwhite">
                    <img src="https://www.markethub.shell.com/content/dam/opennextgen/shell/onboard/en/Images/Shell_MarketHub_Icons_240x180px(Order_Management)2.png" width="75" height="75"  class="shadow" style="border-radius:100%;"/>
                </span>
                <span class="input-group mb-3 w-50 shadow " >
                    <span class="input-group-text input-circle.input-sm" id="basic-addon1">
                        <img src="data:image/jpeg;base64," width="40" height="40" style="border-radius:100%"/>
                    </span>
                    <input type="text" class="form-control " placeholder="Search" aria-label="Search" aria-describedby="basicadon1" style="position:center"/>
                </span>
                <span>
                    <button type="button" class="btn btn-warning btn-circle.btn-sm shadow" style="font-family:Arial; font-weight:400; border-radius: 15px; box-shadow:inset;">Sign in</button>
                </span>
                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav" style="color:ghostwhite;">
                        <li>@Html.ActionLink("Home", "Index", "Home")</li>
                        <li>@Html.ActionLink("About", "About", "Home")</li>
                        <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="container body-content">
            @RenderBody()
            <hr />
            <footer class="footer footer-inverse navbar-fixed-top shadow p-3 mb-5 rounded text-white" style="background-color:deepskyblue;">
                <p style="text-align:center;">&copy; @DateTime.Now.Year </p>
            </footer>
        </div>
        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @RenderSection("scripts", required: false)
    </div>
</body>
</html>
